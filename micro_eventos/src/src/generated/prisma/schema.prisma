generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Eventos {
  id_evento    String   @id @default(uuid())
  id_casa      String
  titulo       String
  descricao    String
  data_inicio  String
  data_fim     String
  idade_minima String
  visibilidade String
  status       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

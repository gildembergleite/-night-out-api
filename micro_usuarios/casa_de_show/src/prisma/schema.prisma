generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_CASA_DE_SHOW")
}

model CasaDeShow {
  id             String   @id @default(uuid())
  artistaId      String
  artista        Artista  @relation(fields: [artistaId], references: [id])

  nome_fantasia  String
  cpf            String
  capacidade     String
  cep            String
  logradouro     String
  numero         String
  complemento    String
  bairro         String
  cidade         String
  uf             String
  geo_lat        String
  geo_lng        String
  created_at     DateTime @default(now())
}